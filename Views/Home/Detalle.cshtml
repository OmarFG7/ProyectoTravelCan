@model List<Viaje>
<style>
    body{
      background-image: url(https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcR5Do3FbKWYX2XO7PyLZmYPCoHGGb7D4z9OOpv0mSwgJmyhnSVw);
    }
    h5{
      font-size: 15px;
    }
</style>
@foreach(var v in Model){
    <img src="@v.Foto" class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">@v.DescripcionCompleta </h5>
        <p class="card-text">Precio:S/.@v.Costo por persona</p>
      </div>
      <ul class="list-group list-group-flush">
        <li class="list-group-item">Salida: @v.nombrePartida</li>
        <li class="list-group-item">Destino: @v.nombreDestino</li>
        <li class="list-group-item">Fecha: @v.Fecha</li>
      </ul>
      <div class="card-body">
        <a asp-action="ReservarViaje" class="btn-primary btn-lg">Somos</a>
    </div>
}